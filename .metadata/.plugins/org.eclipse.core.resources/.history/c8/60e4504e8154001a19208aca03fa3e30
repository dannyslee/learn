package learn.woniuxy.anno;

import java.util.List;

public class CreateTest {
	//获取所有类名
	<T> List<T> findClass(String file){
		List<T> classNames = null;
		//获取所有class
		return classNames;	
	}
	
	//根据注解判断是否创建该对象
	<T,K> void createClass(List<T> ts,Class<K> cl) {
		for(T t:ts ) {
			boolean declaredAnnotation = t.getClass().isAnnotationPresent(Create.class);
			
		}
		
	}
}
